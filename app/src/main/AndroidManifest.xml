<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shogonir.android_sample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HttpConnector.HttpConnectorActivity" />
        <activity android:name=".ImageItemView.ImageItemViewActivity" />
        <activity android:name=".DragSortList.DragSortListActivity" />
        <activity android:name=".HorizontalScrollAndTab.HorizontalScrollAndTabActivity" />
        <activity android:name=".Calendar.CalendarActivity" />
        <activity android:name=".CustomAdapter.CustomAdapterActivity"></activity>
    </application>

</manifest>